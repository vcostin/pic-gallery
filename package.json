{
  "name": "pic-gallery",
  "version": "0.1.0",
  "private": true,
  "description": "A modern web application for artists to showcase and organize their artwork in customizable galleries",
  "scripts": {
    "dev": "prisma generate && next dev --turbopack",
    "dev:trace": "NEXT_TURBOPACK_TRACING=1 prisma generate && next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "build:webpack": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:all": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "updatedb": "prisma generate && prisma db push",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=lcov",
    "test:e2e": "AUTH_ONLY=1 playwright test",
    "test:e2e:fast": "PLAYWRIGHT_FAST=true playwright test --config=playwright.config.optimized.ts",
    "test:e2e:fail-fast": "PLAYWRIGHT_FAIL_FAST=true playwright test --config=playwright.config.optimized.ts",
    "test:e2e:dev": "PLAYWRIGHT_FAIL_FAST=true PLAYWRIGHT_FAST=true playwright test --config=playwright.config.optimized.ts",
    "test:e2e:optimized": "PLAYWRIGHT_OPTIMIZED=true playwright test --config=playwright.config.optimized.ts",
    "test:e2e:perf": "PLAYWRIGHT_PERF_LOG=true PLAYWRIGHT_OPTIMIZED=true playwright test --config=playwright.config.optimized.ts",
    "test:e2e:parallel": "PLAYWRIGHT_FAST=true PLAYWRIGHT_SHARED_DATA=true playwright test --config=playwright.config.optimized.ts",
    "test:e2e:profile": "PLAYWRIGHT_FAST=true playwright test --config=playwright.config.optimized.ts --reporter=line",
    "test:e2e:validate": "node scripts/validate-config.js",
    "test:e2e:all": "npm run test:e2e:gallery && npm run test:e2e:authenticated",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "e2e:cleanup": "node scripts/e2e-cleanup.js",
    "e2e:cleanup:all": "node scripts/e2e-cleanup.js --delete-user",
    "e2e:cleanup:screenshots": "./scripts/cleanup-screenshots.sh",
    "test:e2e:report": "playwright show-report",
    "test:e2e:auth": "playwright test e2e-tests/auth.spec.ts --project=chromium",
    "test:e2e:gallery": "SKIP_AUTH=1 playwright test e2e-tests/gallery.spec.ts --project=chromium",
    "test:e2e:authenticated": "AUTH_ONLY=1 playwright test e2e-tests/authenticated.spec.ts --project=authenticated"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@jest/globals": "^30.0.0-beta.3",
    "@prisma/client": "^6.8.2",
    "@tailwindcss/postcss": "^4.1.8",
    "@uploadthing/react": "^7.3.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.15.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.56.4",
    "tailwind-merge": "^3.3.0",
    "uploadthing": "^7.6.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "^15.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "prisma": "^6.8.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
